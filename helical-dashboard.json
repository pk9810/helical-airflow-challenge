{
    "id": null,
    "uid": null,
    "title": "Helical – Airflow & Containers",
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "editable": true,
    "style": "dark",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    },
    "tags": ["airflow", "prometheus", "cadvisor", "helical"],
    "refresh": "15s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Airflow – Scheduler Heartbeat",
        "datasource": {
          "type": "prometheus",
          "uid": "PROMETHEUS_DS"
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "refId": "A",
            "expr": "rate(airflow_scheduler_heartbeat[5m])",
            "legendFormat": "scheduler_heartbeat"
          }
        ],
        "lines": true,
        "linewidth": 2,
        "fill": 1,
        "tooltip": { "shared": true, "value_type": "individual" },
        "yaxes": [
          { "format": "ops", "logBase": 1 },
          { "format": "short", "logBase": 1 }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Airflow – DAG Runs / DAG",
        "datasource": {
          "type": "prometheus",
          "uid": "PROMETHEUS_DS"
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "refId": "A",
            "expr": "rate(airflow_dagrun_successes_total[5m])",
            "legendFormat": "{{dag_id}} – successes"
          },
          {
            "refId": "B",
            "expr": "rate(airflow_dagrun_failures_total[5m])",
            "legendFormat": "{{dag_id}} – failures"
          }
        ],
        "lines": true,
        "linewidth": 2,
        "fill": 1,
        "tooltip": { "shared": true },
        "yaxes": [
          { "format": "ops", "logBase": 1 },
          { "format": "short", "logBase": 1 }
        ]
      },
      {
        "id": 3,
        "type": "graph",
        "title": "Airflow – Task Duration (seconds)",
        "datasource": {
          "type": "prometheus",
          "uid": "PROMETHEUS_DS"
        },
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
        "targets": [
          {
            "refId": "A",
            "expr": "airflow_task_duration{task_id!=\"\",dag_id!=\"\"}",
            "legendFormat": "{{dag_id}}.{{task_id}}"
          }
        ],
        "lines": true,
        "linewidth": 2,
        "fill": 1,
        "tooltip": { "shared": true },
        "yaxes": [
          { "format": "s", "logBase": 1 },
          { "format": "short", "logBase": 1 }
        ]
      },
      {
        "id": 4,
        "type": "graph",
        "title": "Containers – CPU Usage (seconds/s)",
        "datasource": {
          "type": "prometheus",
          "uid": "PROMETHEUS_DS"
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "refId": "A",
            "expr": "sum by (name) (rate(container_cpu_usage_seconds_total{image!=\"\",name!=\"\"}[5m]))",
            "legendFormat": "{{name}}"
          }
        ],
        "lines": true,
        "linewidth": 2,
        "fill": 1,
        "tooltip": { "shared": true },
        "yaxes": [
          { "format": "ops", "logBase": 1 },
          { "format": "short", "logBase": 1 }
        ]
      },
      {
        "id": 5,
        "type": "graph",
        "title": "Containers – Memory Usage (MB)",
        "datasource": {
          "type": "prometheus",
          "uid": "PROMETHEUS_DS"
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "targets": [
          {
            "refId": "A",
            "expr": "container_memory_usage_bytes{image!=\"\",name!=\"\"} / 1024 / 1024",
            "legendFormat": "{{name}}"
          }
        ],
        "lines": true,
        "linewidth": 2,
        "fill": 1,
        "tooltip": { "shared": true },
        "yaxes": [
          { "format": "megabytes", "logBase": 1 },
          { "format": "short", "logBase": 1 }
        ]
      }
    ]
  }
  